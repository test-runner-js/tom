{
  "name": "test-object-model",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "0.6.1",
  "description": "test-object-model",
  "repository": "https://github.com/test-runner-js/test-object-model",
  "main": "./index.mjs",
  "exports": "./index.mjs",
  "module": "./index.mjs",
  "keywords": [
    "test-runner",
    "test",
    "testing",
    "suite",
    "runner",
    "model"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "npm run dist && npm run test:esm && npm run test:web",
    "test:esm": "test-runner --view.hide-skips test/*.mjs",
    "test:web": "web-runner test/*.mjs",
    "dist": "rollup -c",
    "docs": "jsdoc2md -c jsdoc.conf index.mjs lib/*.mjs > docs/API.md",
    "c8": "c8 --reporter=html npm test"
  },
  "dependencies": {
    "composite-class": "^2.0.1",
    "create-mixin": "^3.0.0",
    "fsm-base": "^0.5.2",
    "race-timeout-anywhere": "^1.0.1",
    "typical": "^6.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@test-runner/web": "^0.3.5",
    "c8": "^7.1.2",
    "isomorphic-assert": "^0.1.1",
    "jsdoc-to-markdown": "^5.0.3",
    "rollup": "^2.11.2",
    "sleep-anywhere": "^1.1.3",
    "test-runner": "^0.8.14"
  },
  "standard": {
    "ignore": [
      "tmp",
      "dist"
    ]
  },
  "files": [
    "index.mjs",
    "lib/*.mjs",
    "dist"
  ]
}

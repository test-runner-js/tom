{
  "name": "test-object-model",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "0.4.0",
  "description": "test-object-model",
  "repository": "https://github.com/test-runner-js/test-object-model",
  "main": "dist/index.js",
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "npm run test:js && npm run test:esm && npm run test:esm-dist",
    "test:js": "rollup -c build/tests.config.js && node tmp/test/tests.js",
    "test:esm": "node --experimental-modules test/tests.mjs",
    "test:esm-dist": "rollup -c build/tests-esm.config.js && node --experimental-modules tmp/test/tests.mjs",
    "dist": "rollup -c build/index.config.js && rollup -c build/index-esm.config.js",
    "docs": "jsdoc2md -c build/jsdoc.conf index.mjs lib/*.mjs > docs/API.md"
  },
  "dependencies": {
    "composite-class": "^0.3.0",
    "create-mixin": "^1.1.1",
    "fsm-base": "^0.4.4",
    "race-timeout-anywhere": "^0.1.1",
    "typical": "^5.0.0"
  },
  "devDependencies": {
    "rollup": "^1.10.1",
    "sleep-anywhere": "^1.1.2",
    "test-runner": "^0.6.0-15"
  },
  "standard": {
    "ignore": [
      "tmp",
      "dist"
    ]
  },
  "files": [
    "index.mjs",
    "lib/*.mjs",
    "dist/index*"
  ]
}
